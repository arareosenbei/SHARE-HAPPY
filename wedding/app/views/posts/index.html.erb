<div class="container">
  <div　class="row">
    <div class="">
      <h1>
        結婚式を見る
      </h1>
    </div>
    <div class="row">
      <p><%= link_to '結婚式を投稿', new_post_path, class: 'btn btn-primary', "data-turbolinks": false %></p>
      <% @posts.each do |post| %>
        <%= link_to post_path(post) do %>
          <p><%= attachment_image_tag post, :image, size: "200x200", fallback: "noimage.png", "data-turbolinks": false %></p>
        <% end %>
        <p><%= post.title %></p>
        <% end %>
    </div>
  </div>
</div>